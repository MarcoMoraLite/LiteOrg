<odoo>
  <data>
    <record model="ir.ui.view" id="valida_curp.list">
      <field name="name">valida_curp list</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <form string="Validador de cedula">
            <header >
                <button name="send_email_template" type="object" string="Enviar Correo" />
                <button name="comprobar" type="object" states="cedula" string="Validar Cedula" />
                <button name="confirmarCed" type="object" states="cedula" string="Confirmar Cédula"/>
                <button name="comprobar2" type="object" states="ine" string="Validar INE"/>
                <button name="confirmarIne" type="object" states="ine" string="Confirmar"/>
                <button name="selfie" type="object" states="foto" string="Validar Foto"/>
                <button name="confirmarSelfie" type="object" states="foto" string="Confirmar Foto"/>
                <button name="guardaContacto" type="object" states="guardar" string="Guardar Contacto"/>
                <field name="state" widget="statusbar" statusbar_visible="ine,cedula,foto,guardar"/>
            </header>
            <sheet>
                <group col="4">
                  <field name="ine" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2"/>
                  <field name="response2" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2"
                         readonly="1"/>
                  <field name="cedula" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2"/>
                  <field name="response" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2" readonly="1"/>
                  <field name="ine_foto" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2"/>
                  <field name="response3" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2" readonly="1"/>
                  <field name="curp" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="fechaNacimiento" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="primerApellido" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="segundoApellido" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="nombres" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="sexo" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="calle" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="colonia" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="codigo_postal" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="ciudad" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="subTipo" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="claveElector" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="registro" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="estado" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="municipio" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="seccion" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="localidad" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="emision" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="vigencia" attrs="{'invisible':[['state','not in',['ine','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="primerApellidoCedula" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="segundoApellidoCedula" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="nombresCedula" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="institucion" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="tipo_cedula" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="titulo" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="estatus_cedula" attrs="{'invisible':[['state','not in',['cedula','guardar']]]}" colspam="2" readonly="1"/>
                  <field name="id_contacto" colspam="2" readonly="1"/>
                  <field name="intentos" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2" readonly="1"/>
                  <field name="intentos_ine" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2" readonly="1"/>
                  <field name="intentos_cedula" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2" readonly="1"/>
                  <field name="bool_ine" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2" readonly="1"/>
                  <field name="bool_ced" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2" readonly="1"/>
                  <field name="bool_foto" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2" readonly="1"/>
                  <field name="noti_ine" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2" readonly="1"/>
                  <field name="noti_ced" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2" readonly="1"/>
                  <field name="noti_foto" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2" readonly="1"/>
                </group>
                <notebook>
                    <page string="INE" name="ine_view">
                        <group>
                            <field name="ine"/>
                            <field name="response2"/>
                            <field name="curp"/>
                            <field name="fechaNacimiento"/>
                            <field name="primerApellido"/>
                            <field name="segundoApellido"/>
                            <field name="nombres"/>
                            <field name="sexo"/>
                            <field name="calle"/>
                            <field name="colonia"/>
                            <field name="codigo_postal"/>
                            <field name="ciudad"/>
                            <field name="subTipo"/>
                            <field name="claveElector"/>
                            <field name="colonia"/>
                            <field name="registro"/>
                            <field name="estado"/>
                            <field name="municipio"/>
                            <field name="seccion"/>
                            <field name="localidad"/>
                            <field name="emision"/>
                            <field name="vigencia"/>
                            <field name="intentos_ine"/>
                            <field name="bool_ine"/>
                            <field name="noti_ine"/>
                        </group>
                    </page>
                    <page string="Cédula" name="ced_view">
                        <group>
                            <field name="cedula"/>
                            <field name="response"/>
                            <field name="primerApellidoCedula"/>
                            <field name="segundoApellidoCedula"/>
                            <field name="nombresCedula"/>
                            <field name="institucion"/>
                            <field name="tipo_cedula"/>
                            <field name="titulo"/>
                            <field name="estatus_cedula"/>
                            <field name="intentos_cedula"/>
                            <field name="bool_ced"/>
                            <field name="noti_ced"/>
                        </group>
                    </page>
                    <page string="Selfie" name="foto_view">
                        <group>
                            <field name="ine_foto"/>
                            <field name="response3"/>
                            <field name="intentos"/>
                            <field name="bool_foto"/>
                            <field name="noti_foto"/>
                        </group>
                    </page>
                </notebook>
            </sheet>
            
        </form>
      </field>
    </record>
      
    <record model="ir.ui.view" id="valida_curp.tree">
      <field name="name">valida_curp tree</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <tree>
            <field name="primerApellido"/>
            <field name="primerApellido"/>
            <field name="primerApellido"/>
            <field name="titulo"/>
            <field name="institucion"/>
            <field name="ciudad"/>
            <field name="codigo_postal"/>
        </tree>
      </field>
    </record>
      
     <record model="ir.ui.view" id="valida_curp.tree2">
      <field name="name">valida_curp datos</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <tree>
            <field name="ine"/>
            <field name="ine_foto"/>
            <field name="cedula"/>
            <field name="nombres"/>
            <field name="state"/>
        </tree>
      </field>
    </record>
      
    <record model="ir.actions.act_window" id="valida_curp.action_window">
      <field name="name">Datos de especialistas</field>
      <field name="res_model">valida_curp.valida_curp</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="valida_curp.tree"/>
    </record>
      
    <record model="ir.actions.act_window" id="valida_curp.action_window2">
      <field name="name">Datos de especialistas</field>
      <field name="res_model">valida_curp.valida_curp</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="valida_curp.tree2"/>
    </record>
      

       <!-- Aqui van los modulos-->    
    <menuitem name="valida_curp" id="valida_curp.menu_root"/>


    <menuitem name="CEDULA" id="valida_curp.menu_1" parent="valida_curp.menu_root"/>
      

    <menuitem name="Datos especialistas" id="valida_curp.menu_1_list" parent="valida_curp.menu_1"
              action="valida_curp.action_window"/>
      
    <menuitem name="Documentos" id="valida_curp.menu_2_list" parent="valida_curp.menu_1"
              action="valida_curp.action_window2"/>

  </data>
</odoo>