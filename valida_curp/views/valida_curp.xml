<odoo>
  <data>
    <record model="ir.ui.view" id="valida_curp.list">
      <field name="name">valida_curp list</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <form string="Validador de CURP">
            <header >
                <button name='comprobar' type="object" string="Validar"/>
            </header>
            <sheet>
                <group col="4">
                  <field name="curp" colspam="2"/>
                  <field name="response" colspam="2"/>
                </group>
            </sheet>
        </form>
      </field>
    </record>
      
    <record model="ir.actions.act_window" id="valida_curp.action_window">
      <field name="name">valida_curp window</field>
      <field name="res_model">valida_curp.valida_curp</field>
      <field name="view_mode">tree,form</field>
    </record>
      
 <!-- Desde aqui lo nuevo-->     
    <record model="ir.ui.view" id="valida_ine.list">
      <field name="name">valida_ine list</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <form string="Validador de INE">
            <header >
                <button name='comprobar_ine' type="object" string="Validar"/>
            </header>
            <sheet>
                <group col="6">
                  <field name="cic" colspam="2"/>
                  <field name="identificadorCiudadano" colspam="2"/>
                  <field name="response2" colspam="2"/>
                </group>
            </sheet>
        </form>
      </field>
    </record>
      
    <record model="ir.actions.act_window" id="valida_ine.action_window">
      <field name="name">valida_ine window</field>
      <field name="res_model">valida_curp.valida_curp</field>
      <field name="view_mode">tree,form</field>
    </record>
      

       <!-- Aqui van los modulos-->    
    <menuitem name="valida_curp" id="valida_curp.menu_root"/>


    <menuitem name="CURP" id="valida_curp.menu_1" parent="valida_curp.menu_root"/>
    <menuitem name="INE" id="valida_ine.menu_1" parent="valida_curp.menu_root"/>
      

    <menuitem name="List" id="valida_curp.menu_1_list" parent="valida_curp.menu_1"
              action="valida_curp.action_window"/>
      
    <menuitem name="List" id="valida_ine.menu_1_list" parent="valida_ine.menu_1"
              action="valida_ine.action_window"/>

  </data>
</odoo>