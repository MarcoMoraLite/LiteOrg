<odoo>
  <data>
    <record model="ir.ui.view" id="valida_curp.list">
      <field name="name">valida_curp list</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <form string="Validador de cedula">
            <header >
                <button name="comprobar" type="object" states="cedula" string="Validar Cedula" />
                <button name="confirmarCed" type="object" states="cedula" string="Confirmar Cédula"/>
                <button name="comprobar2" type="object" states="ine" string="Validar INE"/>
                <button name="confirmarIne" type="object" states="ine" string="Confirmar"/>
                <button name="selfie" type="object" states="foto" string="Validar Foto"/>
                <button name="confirmarSelfie" type="object" states="foto" string="Confirmar Foto"/>
                <button name="rechazarContacto" type="object" string="Rechazar"/>
                <field name="state" widget="statusbar" statusbar_visible="ine,cedula,foto,guardar"/>
            </header>
            <sheet>
                <group col="4">
                  <field name="ine" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2"/>
                  <field name="response2" attrs="{'invisible':[['state','not in',['ine']]]}" colspam="2"
                         readonly="1"/>
                  <field name="cedula" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2"/>
                  <field name="response" attrs="{'invisible':[['state','not in',['cedula']]]}" colspam="2" readonly="1"/>
                  <field name="ine_foto" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2"/>
                  <field name="response3" attrs="{'invisible':[['state','not in',['foto']]]}" colspam="2" readonly="1"/>
                  <field name="estatus_gen" colspam="2" readonly="1"/>
                </group>
                <notebook>
                    <page string="INE" name="ine_view">
                        <group>
                            <field name="response2" readonly="1"/>
                            <field name="curp" readonly="1"/>
                            <field name="fechaNacimiento" readonly="1"/>
                            <field name="primerApellido" readonly="1"/>
                            <field name="segundoApellido" readonly="1"/>
                            <field name="nombres" readonly="1"/>
                            <field name="sexo" readonly="1"/>
                            <field name="calle" readonly="1"/>
                            <field name="colonia" readonly="1"/>
                            <field name="codigo_postal" readonly="1"/>
                            <field name="ciudad" readonly="1"/>
                            <field name="subTipo" readonly="1"/>
                            <field name="claveElector" readonly="1"/>
                            <field name="colonia" readonly="1"/>
                            <field name="registro" readonly="1"/>
                            <field name="estado" readonly="1"/>
                            <field name="municipio" readonly="1"/>
                            <field name="seccion" readonly="1"/>
                            <field name="localidad" readonly="1"/>
                            <field name="emision" readonly="1"/>
                            <field name="vigencia" readonly="1"/>
                            <field name="intentos_ine" readonly="1"/>
                            <field name="bool_ine" readonly="1"/>
                            <field name="noti_ine" readonly="1"/>
                        </group>
                    </page>
                    <page string="Cédula" name="ced_view">
                        <group>
                            <field name="response" readonly="1"/>
                            <field name="primerApellidoCedula" readonly="1"/>
                            <field name="segundoApellidoCedula" readonly="1"/>
                            <field name="nombresCedula" readonly="1"/>
                            <field name="institucion" readonly="1"/>
                            <field name="tipo_cedula" readonly="1"/>
                            <field name="titulo" readonly="1"/>
                            <field name="estatus_cedula" readonly="1"/>
                            <field name="intentos_cedula" readonly="1"/>
                            <field name="bool_ced" readonly="1"/>
                            <field name="noti_ced" readonly="1"/>
                        </group>
                    </page>
                    <page string="Selfie" name="foto_view">
                        <group>
                            <field name="response3" readonly="1"/>
                            <field name="intentos" readonly="1"/>
                            <field name="bool_foto" readonly="1"/>
                            <field name="noti_foto" readonly="1"/>
                        </group>
                    </page>
                </notebook>
            </sheet>
            
        </form>
      </field>
    </record>
      
    <record model="ir.ui.view" id="valida_curp.tree">
      <field name="name">valida_curp tree</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <tree>
            <field name="nombres"/>
            <field name="primerApellido"/>
            <field name="segundoApellido"/>
            <field name="estado"/>
            <field name="ciudad"/>
            <field name="codigo_postal"/>
            <field name="sexo"/>
            <field name="titulo"/>
            <field name="institucion"/>
            <field name="primerApellidoCedula"/>
            <field name="segundoApellidoCedula"/>
            <field name="state"/>
        </tree>
      </field>
    </record>
      
     <record model="ir.ui.view" id="valida_curp.tree2">
      <field name="name">valida_curp datos</field>
      <field name="model">valida_curp.valida_curp</field>
      <field name="arch" type="xml">
        <tree>
            <field name="nombres"/>
            <field name="primerApellido"/>
            <field name="state"/>
            <field name="ine_fname" invisible="1" />
            <field name="ine" widget="binary" filename="ine_fname" />
            <field name="cedula"/>
            <field name="selfie_fname" invisible="1" />
            <field name="ine_foto" widget="binary" filename="selfie_fname" />
        </tree>
      </field>
    </record>
      
    <record model="ir.actions.act_window" id="valida_curp.action_window">
      <field name="name">Datos de especialistas</field>
      <field name="res_model">valida_curp.valida_curp</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="valida_curp.tree"/>
    </record>
      
    <record model="ir.actions.act_window" id="valida_curp.action_window2">
      <field name="name">Datos de especialistas</field>
      <field name="res_model">valida_curp.valida_curp</field>
      <field name="view_mode">tree,form</field>
      <field name="view_id" ref="valida_curp.tree2"/>
    </record>
      

       <!-- Aqui van los modulos-->    
    <menuitem name="Validación Especialistas" id="valida_curp.menu_root"/>


    <menuitem name="Reportes" id="valida_curp.menu_1" parent="valida_curp.menu_root"/>
      

    <menuitem name="Datos especialistas" id="valida_curp.menu_1_list" parent="valida_curp.menu_1"
              action="valida_curp.action_window"/>
      
    <menuitem name="Documentos" id="valida_curp.menu_2_list" parent="valida_curp.menu_1"
              action="valida_curp.action_window2"/>

  </data>
</odoo>